          "path": ["value", "invoiceNumber"]
        },
        "stagger": {
          "ms": {
            "_gen": "uniformDistribution",
            "bounds": [60000, 300000]  // 1 to 5 minutes in milliseconds
          }
        }
      },
      "value": {
