          "_gen": "var",
          "var": "forkKey"
        },
        "payDateTime_Ltz": {
          "_gen": "now"
        },
        "payTimestamp_Epoc": {
          "_gen": "epochTime"
        },
        "paid": {
          "_gen": "lookup",
          "topic": "salesbaskets",
          "path": ["value", "total"]
        },
        "finTransactionId": {
